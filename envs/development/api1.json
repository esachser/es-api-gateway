{
    "transports": [
        {
            "type": "EsHttpTransport",
            "id": "http",
            "parameters": {
                "routeContext": "/api/teste/",
                "routes": {
                    "/": ["GET", "POST"],
                    "/healthz": ["GET"]
                }
            },
            "mids": [
                {
                    "type": "EsPropertyMiddleware",
                    "data": {
                        "name": "response.headers.responsetime",
                        "expression": "ctx.properties.metrics.timems"
                    },
                    "after": true
                },
                {
                    "type": "EsMetricsMiddleware",
                    "data":{
                        "prop": "metrics.timems"
                    }
                },
                {
                    "type": "EsParallelMiddleware",
                    "data": {
                        "mids": [
                            [
                                {
                                    "type": "EsPropertyMiddleware",
                                    "data": {
                                        "name": "response.headers.pa",
                                        "expression": "Date.now()"
                                    },
                                    "after": true
                                }
                            ],
                            [
                                {
                                    "type": "EsPropertyMiddleware",
                                    "data": {
                                        "name": "response.headers.pb",
                                        "expression": "Date.now()"
                                    },
                                    "after": true
                                }
                            ]
                        ]
                    },
                    "after": true
                },
                {
                    "type": "EsPropertyMiddleware",
                    "data": {
                        "name": "response.headers.responsetest",
                        "value": "testandoResposta"
                    },
                    "after": true
                },
                {
                    "type": "EsPropertyMiddleware",
                    "data": {
                        "name": "response.body",
                        "value": {
                            "ok": true
                        }
                    },
                    "after": true
                },
                {
                    "type": "EsPropertyMiddleware",
                    "data": {
                        "name": "response.status",
                        "value": 200
                    },
                    "after": true
                }
            ]
        }
    ],
    "execution": [
        {
            "type": "EsPropertyMiddleware",
            "data": {
                "name": "headers.test",
                "value": "testando"
            },
            "after": false
        }
    ]
}