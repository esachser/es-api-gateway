{
    "transports": [
        {
            "type": "EsHttpTransport",
            "id": "http",
            "parameters": {
                "routeContext": "/api/teste/",
                "routes": {
                    "/": ["GET", "POST"],
                    "/healthz": ["GET"]
                }
            },
            "mids": [
                {
                    "type": "EsPropertyMiddleware",
                    "data": {
                        "name": "response.headers.responsetime",
                        "expression": "props.metrics.timems",
                        "runAfter": true
                    }
                },
                {
                    "type": "EsMetricsMiddleware",
                    "data":{
                        "prop": "metrics.timems"
                    }
                },
                {
                    "type": "EsParallelMiddleware",
                    "data": {
                        "mids": [
                            [
                                {
                                    "type": "EsPropertyMiddleware",
                                    "data": {
                                        "name": "response.headers.pa",
                                        "expression": "Date.now()",
                                        "runAfter": true
                                    }
                                }
                            ],
                            [
                                {
                                    "type": "EsPropertyMiddleware",
                                    "data": {
                                        "name": "response.headers.pb",
                                        "expression": "Date.now()",
                                        "runAfter": true
                                    }
                                }
                            ]
                        ],
                        "runAfter": true
                    }
                },
                {
                    "type": "EsPropertyMiddleware",
                    "data": {
                        "name": "response.headers.responsetest",
                        "value": "testandoResposta",
                        "runAfter": true
                    }
                },
                {
                    "type": "EsPropertyMiddleware",
                    "data": {
                        "name": "response.body",
                        "value": {
                            "ok": true
                        },
                        "runAfter": true
                    }
                },
                {
                    "type": "EsPropertyMiddleware",
                    "data": {
                        "name": "response.status",
                        "value": 200,
                        "runAfter": true
                    }
                }
            ]
        }
    ],
    "execution": [
        {
            "type": "EsPropertyMiddleware",
            "data": {
                "name": "headers.test",
                "value": "testando",
                "runAfter": false
            }
        }
    ]
}