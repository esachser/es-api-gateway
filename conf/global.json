{
    "env": "development",
    "httpPort": 4000,
    "logLevel": "info",
    "authenticators": [
        {
            "type": "EsOAuth2JwtAuthenticator",
            "id": "auth0authesachser",
            "name": "Auth0 Esachser Authenticator",
            "parameters": {
                "jwksUri": "https://esachser.auth0.com/.well-known/jwks.json",
                "scopesProp": "scope",
                "issuer": "https://esachser.auth0.com/",
                "audience": "https://authtestapi"
            }
        }
    ]
}